
<p>Landing Page Content will go here. You should only see this if you aren't logged in.</p>


<div id="teaser">
<%= video_tag("Group1Project3.mp4", size: "500x500", :autoplay => true) %>
</div>

<div id="landing_table">
    <table>
        <tr id="tblHead">
            <th>Rank</th>
            <th>Player Name</th>
            <th>Score</th>

        </tr>
        <% @scores2.each do |score| %>
          <tr>
            <td><%= @i %></td>
            <td><%= score.name %></td>
            <td><%= score.score %></td>

          </tr>

          <% @i = @i + 1 %>
        <% end %>
    </table>
</div>
